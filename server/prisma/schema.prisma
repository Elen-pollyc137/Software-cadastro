generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id              String            @id @default(uuid())
  name            String
  email           String            @unique
  password        Int
  token           String
  Familyreference Familyreference[]
}

model Familyreference {
  id             String   @id @default(uuid())
  userId         String
  name           String
  cpf            String
  nis            String
  rg             String
  titulo         String
  naturalness    String
  nameMother     String
  nameFather     String
  birthdayDate   String
  surname        String
  phoneNumber    String
  matrialStatus  String
  education      String
  elderlyPerson  String
  disabledPerson String
  folderNunber   String
  createdAt      DateTime @default(now())
  isPublic       Boolean  @default(false)
  User           User     @relation(fields: [userId], references: [id])
}
